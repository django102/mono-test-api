{
  "name": "mono-test-api",
  "version": "1.0.0",
  "description": "Mono Assessment Test, Backend API",
  "main": "src/app.ts",
  "repository": "https://github.com/django102/mono-test-api.git",
  "author": "Wale K. Santos <eighteen.eighty.one@outlook.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "nodemon",
    "build": "node package-build.js",
    "start": "yarn build && node ./dist/app.js",
    "lint": "tslint -c tslint.json --fix \"src/**/*.ts\"",
    "ts:watch": "tsc -w",
    "test": "jest --verbose --runInBand --coverage --collectCoverageFrom=src/api/**/*.ts --forceExit",
    "deploy": "yarn"
  },
  "dependencies": {
    "@redis/json": "^1.0.7",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "body-parser": "^1.20.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "moment": "^2.30.1",
    "mongodb": "^6.9.0",
    "mongoose": "^8.6.2",
    "morgan": "^1.10.0",
    "ncp": "^2.0.0",
    "node-cron": "^3.0.3",
    "nps": "^5.10.0",
    "nps-utils": "^1.7.0",
    "querystring": "^0.2.1",
    "redis": "^4.7.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "winston": "^3.14.2",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "istanbul": "^0.4.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "rootDir": ".",
    "testRegex": "./test/.*test.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.ts"
    ]
  }
}
